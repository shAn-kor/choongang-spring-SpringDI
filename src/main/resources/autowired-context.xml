<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<!--  의존 객체 자동주입 명령(어노테이션) 활성화  -->
    <context:annotation-config/>
    <bean id="printer" class="ex03.autowired.Printer"/>
    <bean id="doc" class="ex03.autowired.Document"/>
    <bean id="doc2" class="ex03.autowired.Document"/>

    <bean id="computer" class="ex04.Computer"/>
    <bean id="monitor" class="ex04.Monitor"/>
    <bean id="mouse" class="ex04.Mouse"/>
    <bean id="keyboard" class="ex04.Keyboard"/>

</beans>